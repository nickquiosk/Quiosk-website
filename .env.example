PORT=8000

# Google Maps key used by kiosk-finder.html
GOOGLE_MAPS_API_KEY=

# Google Business Profile API OAuth (user-based)
GBP_CLIENT_ID=
GBP_CLIENT_SECRET=
GBP_REFRESH_TOKEN=

# Optional: if empty, server picks first available account
GBP_ACCOUNT_ID=

# Comma-separated list of origins that may call /api/locations
# Example: https://nickquiosk.github.io,https://nickquiosk.github.io/Quiosk-website,http://localhost:8000
ALLOWED_ORIGINS=

# Manual location database (JSON file path, relative to project root)
DATA_FILE=data/locations.json

# Required token for importing locations via /api/import-locations
IMPORT_TOKEN=
